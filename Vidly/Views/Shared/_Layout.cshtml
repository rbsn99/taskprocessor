
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My mgr</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <style>
        .node circle {
            fill: #fff;
            stroke: steelblue;
            stroke-width: 3px;
        }

        .node text {
            font: 12px sans-serif;
        }

        .link {
            fill: none;
            stroke: #ccc;
            stroke-width: 2px;
        }
    </style>
</head>
<body>
    @Html.Partial("_NavBar")
    <div class="wrapper container">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Master's Degree - Task processor application - Mateusz Rybczynski</p>
        </footer>
    </div>

 
    @Scripts.Render("~/bundles/lib")
    @RenderSection("scripts", required: false)
    @RenderSection("flowdiagram", required: false)
</body>
</html>
